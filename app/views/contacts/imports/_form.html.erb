<%= form_with(url: contact_imports_path, method: "post",id: "import_form",class: "contents") do |form| %>

  <div class="my-5">
    <%= form.label :import_file %>
    <%= form.file_field :import_file, required: true, class: "block shadow rounded-md border border-gray-200 outline-none px-3 py-2 mt-2 w-full" %>
  </div>

  <div class="inline">
    <%= form.submit "Import Contacts", class: "rounded-lg py-3 px-5 bg-blue-600 text-white inline-block font-medium cursor-pointer" %>
  </div>
<% end %>
